<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.edcan.exampleapplication">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.EditTextActivity" />
        <activity android:name=".activity.InflateActivity" />
        <activity android:name=".activity.StartActivityResultActivity" />
        <activity android:name=".activity.MenuActivity" />
        <activity android:name=".activity.CallActivity" />
        <activity android:name=".activity.ParcelableExamMenuActivity">

        </activity>
        <activity android:name=".activity.ParcelableExamResultActivity" />
        <activity android:name=".activity.ActivityFlowTestActivity" />
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AfterLoginActivity" />
        <activity android:name=".activity.ClassSelectActivity"></activity>
    </application>

</manifest>